{
  "executable": "API",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "API",
  "author": "",
  "ignoreAllCertificateErrors":true,
  "singleInstance": false,
  "environment": "Development",
  "aspCoreBackendPort": 5001,
  "build": {
    "appId": "com.API.app",
    "productName": "API",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      },
      {
        "from": "./Certificates",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      {
        "from": "./Certificates",
        "to": "Certtificates",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}