{
  "executable": "API",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "UnifiedEdge",
  "author": "Dalibor Kundrat",
  "ignoreAllCertificateErrors":true,
  "singleInstance": false,
  "environment": "Production",
  "aspCoreBackendPort": 5001,
  "build": {
    "appId": "github.com/damikun/UnifiedEdge",
    "productName": "UnifiedEdge",
    "copyright": "Copyright Â© 2022",
    "buildVersion": "1.0.1",
    "compression": "maximum",
    "directories": {
      "output": "../../../Artifacts"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      },
      {
        "from": "./bin/DevCerts",
        "to": "bin/DevCerts",
        "filter": [ "**/*" ]
      },
      {
        "from": "./DevCerts",
        "to": "DevCerts",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      {
        "from": "./Certificates",
        "to": "Certtificates",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}